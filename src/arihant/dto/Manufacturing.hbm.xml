<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="arihant.dto">
  <class name="Manufacturing" table="manufacturinginfo">
    <id name="receiptNo"/>
    <property name="marketingUserID"/>
    <property name="companyName"/>
    <property name="address"/>
    <property name="contactNo"/>
    <property name="transformerType"/>
    <property name="manufacturingDesign"/>
    <property name="materialUsed"/>
    <property name="manufacturingProcesses"/>
    <property name="manufacturingCost"/>
        
  </class>
</hibernate-mapping>
